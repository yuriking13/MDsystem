{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean:js-mirrors": "node scripts/clean-js-mirrors.mjs",
    "check:js-mirrors": "node scripts/clean-js-mirrors.mjs --check",
    "build": "pnpm run clean:js-mirrors && tsc -p tsconfig.json --noEmit && vite build",
    "typecheck": "pnpm run clean:js-mirrors && tsc -p tsconfig.json --noEmit",
    "preview": "vite preview",
    "lint": "pnpm run clean:js-mirrors && eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "pnpm run clean:js-mirrors && eslint . --ext .ts,.tsx --fix --max-warnings 0",
    "test": "pnpm run clean:js-mirrors && vitest run",
    "test:responsive": "node scripts/run-responsive-suite.mjs",
    "test:watch": "vitest",
    "test:coverage": "pnpm run clean:js-mirrors && vitest run --coverage"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@sentry/react": "^10.39.0",
    "@sgratzl/chartjs-chart-boxplot": "^4.4.5",
