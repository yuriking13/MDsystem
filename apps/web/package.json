    "@tiptap/extension-font-family": "^3.14.0",
    "@tiptap/extension-highlight": "^3.14.0",
    "@tiptap/extension-image": "^3.14.0",
    "@tiptap/extension-link": "^3.14.0",
    "@tiptap/extension-placeholder": "^3.14.0",
    "@tiptap/extension-table": "^3.14.0",
    "@tiptap/extension-table-cell": "^3.14.0",
    "@tiptap/extension-table-header": "^3.14.0",
    "@tiptap/extension-table-row": "^3.14.0",
    "@tiptap/extension-text-align": "^3.14.0",
    "@tiptap/extension-text-style": "^3.14.0",
    "@tiptap/extension-underline": "^3.14.0",
    "@tiptap/react": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "chart.js": "^4.5.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dompurify": "^3.3.1",
    "file-saver": "^2.0.5",
    "flowbite": "^4.0.1",
    "flowbite-react": "^0.12.16",
    "flowbite-react-icons": "^1.3.1",
    "lodash-es": "^4.17.23",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-force-graph-2d": "^1.29.0",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^6.30.2",
    "tabulator-tables": "6.3.1",
    "tailwind-merge": "^3.4.0",
    "tiptap-pagination-plus": "^3.0.3",
    "zod": "^3.25.76"
