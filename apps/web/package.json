    "globals": "^17.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.53.1",
    "vite": "^5.4.11",
    "vitest": "^3.2.4"
  }
